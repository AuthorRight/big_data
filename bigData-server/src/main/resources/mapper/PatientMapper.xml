<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bigdata.bigdataserver.dao.PatientDao">

    <select id="queryAll" resultType="patient">
        select * from patient
    </select>

    <select id="countByProvince" resultType="provinceValue">
        select province name, count(*) value from patient group by province
    </select>

</mapper>